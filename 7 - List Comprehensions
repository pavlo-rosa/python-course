a = [1, 4, 9, 16, 25, 36, 49, 64, 81, 100]
b = [item for item in a if item % 2 == 0]

print("Input:", a)
print("Solution:", b)

# Extra Random Seed
print("\nExtra 1")
import random

a = sorted(random.sample(range(100), 10))
b = [item for item in a if item % 2 == 0]

print("Input:", a)
print("Solution:", b)
